package com.hp.test04;
 7 
 8 public class HS_Loop1 {
 9 
10     public static void main(String[] args) {
11         // TODO Auto-generated method stub
12        int i=1,sum=0;
13        
14        while(i<=100){
15            sum=sum+i;
16            i++;
17        }
18        System.out.println("100以内的和："+sum);
19     }
20 
21 }
